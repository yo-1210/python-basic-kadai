        "\n",
        "class Human:\n",
        "\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}, 年齢: {self.age}歳\")\n",
        "\n",
        "\n",
        "person = Human(\"侍太郎\", 25)\n",
        "\n",
        "\n",
        "person.printinfo()\n"
      ],
